# mount point   fstype      device                  [device2] [parameters]
/boot           emmc        /dev/block/mmcblk0p19
/system         ext4        /dev/block/mmcblk0p32
/cache          ext4        /dev/block/mmcblk0p33
/data           ext4        /dev/block/mmcblk0p34
/misc           emmc        /dev/block/mmcblk0p21
/recovery       emmc        /dev/block/mmcblk0p20
/devlog         ext4        /dev/block/mmcblk0p24
/usb-otg        auto        /dev/block/sda1         /dev/block/sda  flags=display="USB OTG";storage;removable
